{
  "name": "@cliqz/adblocker",
  "version": "0.11.0",
  "description": "Cliqz adblocker library",
  "author": "Cliqz",
  "homepage": "https://github.com/cliqz-oss/adblocker#readme",
  "license": "MPL-2.0",
  "main": "build/cjs/adblocker.js",
  "module": "build/es6/adblocker.js",
  "browser": "dist/adblocker.umd.min.js",
  "types": "dist/types/adblocker.d.ts",
  "files": [
    "build",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cliqz-oss/adblocker.git"
  },
  "scripts": {
    "clean": "rm -rfv dist build coverage",
    "lint": "tslint --config ../../tslint.json --project .",
    "build-cjs": "tsc -p . --outDir build/cjs --module commonjs",
    "build-es6": "tsc -p . --outDir build/es6 --module esnext",
    "build": "concurrently 'npm run build-cjs' 'npm run build-es6'",
    "prebuild": "npm run clean",
    "bundle": "rollup --config rollup.config.ts",
    "prebundle": "npm run build",
    "prepack": "npm run bundle",
    "test": "jest --coverage --no-cache ./test/",
    "pretest": "npm run lint",
    "generate-codebooks": "ts-node ./tools/generate_compression_codebooks.ts"
  },
  "bugs": {
    "url": "https://github.com/cliqz-oss/adblocker/issues"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.9.0",
    "@types/chrome": "0.0.86",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.3",
    "concurrently": "^4.1.1",
    "jest": "^24.8.0",
    "rollup": "^1.16.3"
  },
  "dependencies": {
    "tldts": "^5.2.1",
    "tslib": "^1.10.0",
    "tsmaz": "^1.2.2"
  }
}
