{
  "name": "@cliqz/adblocker-webextension-example",
  "version": "0.12.0",
  "description": "Example of WebExtension adblocker using Cliqz",
  "author": "Cliqz",
  "homepage": "https://github.com/cliqz-oss/adblocker#readme",
  "license": "MPL-2.0",
  "main": "lib/adblocker-webextension-example.js",
  "files": [
    "LICENSE",
    "dist",
    "manifest.json"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:cliqz-oss/adblocker.git"
  },
  "scripts": {
    "clean": "rm -rfv dist build coverage",
    "lint": "tslint --config ../../tslint.json --project .",
    "build-cjs": "tsc -p . --outDir build/cjs --module commonjs",
    "build-es6": "tsc -p . --outDir build/es6 --module esnext",
    "build": "concurrently 'npm run build-cjs' 'npm run build-es6'",
    "prebuild": "npm run clean && npm run lint",
    "bundle": "rollup --config rollup.config.ts",
    "prebundle": "npm run build",
    "prepack": "npm run bundle",
    "test": "echo 'ok'"
  },
  "bugs": {
    "url": "https://github.com/cliqz-oss/adblocker/issues"
  },
  "dependencies": {
    "@cliqz/adblocker-webextension": "^0.12.0",
    "@cliqz/adblocker-webextension-cosmetics": "^0.12.0"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.9.0",
    "concurrently": "^4.1.1",
    "rollup": "^1.16.4",
    "rollup-plugin-node-resolve": "^5.2.0"
  }
}
