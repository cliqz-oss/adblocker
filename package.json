{
  "name": "@cliqz/adblocker",
  "version": "0.1.13",
  "description": "Cliqz adblocker library",
  "repository": {
    "type": "git",
    "url": "git@github.com:cliqz-oss/adblocker.git"
  },
  "author": "Cliqz",
  "license": "MPL-2.0",
  "main": "dist/adblocker.umd.min.js",
  "module": "dist/adblocker.es.js",
  "jsnext:main": "dist/adblocker.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "index.ts",
    "src"
  ],
  "scripts": {
    "clean": "rm -rfv dist build",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "build": "tsc -p tsconfig.json --module ES6 --outDir build",
    "watch": "tsc -p tsconfig.json --module ES6 --outDir build --watch",
    "bundle": "rollup -c rollup.config.js && google-closure-compiler --js=./dist/adblocker.umd.js --js_output_file=./dist/adblocker.umd.min.js",
    "pretest": "npm run lint",
    "test": "jest --coverage --no-cache ./test/",
    "dev": "jest --watch ./test/"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.73",
    "@types/jest": "^23.3.0",
    "@types/jsdom": "^11.0.6",
    "@types/node": "^10.9.4",
    "archiver": "^2.1.1",
    "benchmark": "^2.1.4",
    "chalk": "^2.4.1",
    "concurrently": "^3.5.1",
    "docopt": "^0.6.2",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "google-closure-compiler": "^20180805.0.0",
    "jest": "^23.6.0",
    "jsdom": "^11.12.0",
    "rollup": "^0.64.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "selenium-webdriver": "^3.6.0",
    "ts-jest": "^23.10.0-beta.6",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3",
    "ws": "^6.0.0"
  },
  "dependencies": {
    "ad-block": "^4.1.1",
    "cross-fetch": "^2.2.2",
    "tldts": "file:tldts-3.0.0.tgz"
  }
}
