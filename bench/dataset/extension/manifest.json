{
  "description": "Collect All Network Requests",
  "manifest_version": 2,
  "name": "request-collector",
  "version": "1.0",
  "permissions": [
    "webRequest",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Request Collector"
  },
  "content_scripts": []
}
